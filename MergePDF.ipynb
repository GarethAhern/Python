    "#conda install -c conda-forge pypdf2\n",
    "from PyPDF2 import PdfFileMerger\n",
    "\n",
    "pdfs = ['1.pdf', '2.pdf']\n",
    "\n",
    "merger = PdfFileMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(pdf)\n",
    "\n",
    "merger.write(\"result.pdf\")\n",
    "merger.close()"
   ]
